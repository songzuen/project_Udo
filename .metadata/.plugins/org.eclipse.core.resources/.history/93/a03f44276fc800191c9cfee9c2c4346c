package com.wad.udo.rv.domain;

public class RvInfo {

	private int rv_idx;
	private int idx;
	private int rv_date = new WeekInfo().toDateInfo();
	private int rv_time;
	private int rv_bike;
	
	public int getRv_idx() {
		return rv_idx;
	}
	public void setRv_idx(int rv_idx) {
		this.rv_idx = rv_idx;
	}
	public int getIdx() {
		return idx;
	}
	public void setIdx(int idx) {
		this.idx = idx;
	}
	
	public int getRv_time() {
		return rv_time;
	}
	public void setRv_time(int rv_time) {
		this.rv_time = rv_time;
	}
	public int getRv_bike() {
		return rv_bike;
	}
	public void setRv_bike(int rv_bike) {
		this.rv_bike = rv_bike;
	}
	
	public RvInfo(int rv_idx, int idx, int rv_date, int rv_time, int rv_bike) {
		super();
		this.rv_idx = rv_idx;
		this.idx = idx;
		this.rv_date = rv_date;
		this.rv_time = rv_time;
		this.rv_bike = rv_bike;
	}

	//default 생성자
	public RvInfo() {

	}
	
	
}
